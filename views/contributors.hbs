<div class="container contributors-container">
  <div class="row">
    <div class="col s12">
      <h4 class="center white-text light-line">Contributors</h4>
    </div>
    <div class="col s12">
      {{#each this.list}}
        <div class="list-category light-line clearfix">
          <div class="col s12">
            {{#each this}}
              <div class="col s4">
                <div class="card grey darken-4 contributor">
                  <div class="card-content clearfix">
                    <div class="col s12">
                      <div class="col s4 profileImage" style="background-image:url('{{this.image}}')">
                      </div>
                      <div class="col s8 details">
                        <h6 class="name">{{this.name}}</h6>
                        <p class="designation">{{this.designation}}</p>
                        <p class="role">{{this.role}}</p>
                        <!-- <div class="desc">
                          Worked as Heroku developer
                        </div> -->
                        {{#if this.showEmail}}
                          <a class="send-mail" href="mailto:{{this.mail}}"><i class="material-icons">mail</i></a>
                        {{/if}}
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {{/each}}
          </div>
        </div>
      {{/each}}
    </div>
  </div>
</div>
